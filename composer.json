{
    "name": "blackcube/admin",
    "description": "Blackcube CMS Administration system",
    "keywords": ["blackcube", "cms", "admin", "back office", "yii", "yii2", "content management"],
    "homepage": "https://code.redcat.io/blackcube/admin",
    "type": "yii2-extension",
    "license": "proprietary",
    "support": {
        "issues": "https://code.redcat.io/blackcube/admin/issues",
        "wiki": "https://code.redcat.io/blackcube/admin/wikis/home",
        "source": "https://code.redcat.io/blackcube/admin/tree/devel"
    },
    "authors": [
        {
            "name": "Philippe Gaultier",
            "email": "pgaultier@redcat.io"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-dom": "*",
        "blackcube/core": "^3.3.0",
        "hashids/hashids": "~4.1",
        "swaggest/json-schema": "~0.12",
        "vlucas/phpdotenv": "~5.4",
        "yiisoft/yii2-imagine": "~2.3",
        "yiisoft/yii2": "~2.0.13"
    },
    "require-dev" : {
        "codeception/codeception": "~5.0",
        "codeception/module-yii2": "~1.1",
        "codeception/module-asserts": "~2.0",
        "yiisoft/yii2-debug":"~2.1",
        "yiisoft/yii2-gii": "~2.2"
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "blackcube\\admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "post-install-cmd": [
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-devel-3.x": "3.x-dev"
        }
    }
}
